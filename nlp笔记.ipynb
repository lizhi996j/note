   "source": [
    "# 对文本用list()或set()函数获得字母级别文本表示\n",
    "sentence = \"I love python\"\n",
    "words = list(sentence)\n",
    "print(words)\n",
    "\n",
    "# 对文本用split()函数\n",
    "sentence = \"I love python\"\n",
    "words = set(sentence)\n",
    "print(words)\n"
